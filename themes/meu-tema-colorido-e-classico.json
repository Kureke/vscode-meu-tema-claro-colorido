  "tokenColors": [
    {
      "name": "Comentários",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#080"
      }
    },
    {
      "name": "Strings",
      "scope": ["string"],
      "settings": {
        "foreground": "#ff3"
      }
    },
    {
      "name": "Funções",
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#0af"
      }
    },
    {
      "name": "Variáveis",
      "scope": ["variable"],
      "settings": {
        "foreground": "#f5f"
      }
    },
    {
      "name": "Palavras-chave",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#ff0066"
      }
    },
    {
      "name": "Constantes",
      "scope": ["constant"],
      "settings": {
        "foreground": "#fcf"
      }
    },
    {
      "name": "Números",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#b2f"
      }
    },
    {
      "name": "Delimitadores",
      "scope": ["punctuation.definition", "punctuation.separator", "punctuation.terminator"],
      "settings": {
        "foreground": "#fff"
      }
    }
    
  ]
}
