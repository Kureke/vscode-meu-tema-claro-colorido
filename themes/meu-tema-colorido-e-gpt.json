{
  "name": "Tema Escuro Sugerido pela ChatGPT",
  "type": "dark",
  "colors": {
    "activeBorder": "#F39C12",
    "activityBar.background": "#2C3E50",
    "activityBar.foreground": "#ECF0F1",
    "activityBar.hoverBackground": "#34495E",
    "activityBar.inactiveForeground": "#95A5A6",
    "activityBarBadge.background": "#E74C3C",
    "button.background": "#E67E22",
    "button.foreground": "#FFFFFF",
    "dropdown.background": "#34495E",
    "dropdown.border": "#F39C12",
    "dropdown.foreground": "#ECF0F1",
    "editor.background": "#090909",
    "editor.foreground": "#fdf",
    "editor.lineHighlightBackground": "#2a2a2a",
    "editor.selectionBackground": "#037",
    "editorHoverWidget.background": "#2C3E50",
    "editorHoverWidget.foreground": "#ECF0F1",
    "editorHoverWidget.hoverBackground": "#34495E",
    "editorLineNumber.foreground": "#080",
    "editorLineNumber.activeForeground": "#06f",
    "editorSuggestWidget.background": "#34495E",
    "editorSuggestWidget.selectedBackground": "#2C3E50",
    "editorWidget.background": "#2C3E50",
    "editorWidget.hoverBackground": "#34495E",
    "input.background": "#34495E",
    "input.border": "#F39C12",
    "input.foreground": "#ECF0F1",
    "list.activeSelectionBackground": "#E67E22",
    "list.activeSelectionForeground": "#ECF0F1",
    "list.hoverBackground": "#34495E",
    "list.hoverForeground": "#ECF0F1",
    "scrollbarSlider.activeBackground": "#E67E22",
    "scrollbarSlider.background": "#34495E",
    "scrollbarSlider.hoverBackground": "#E67E22",
    "sideBar.background": "#2C3E50",
    "statusBar.background": "#2C3E50",
    "statusBar.foreground": "#ECF0F1",
    "statusBarItem.hoverBackground": "#34495E",
    "editorGroupHeader.tabsBackground": "#2C3E50",
    "tab.activeBackground": "#34495E",
    "tab.activeBorder": "#F39C12",
    "tab.activeForeground": "#ECF0F1",
    "tab.border": "#34495E",
    "tab.inactiveBackground": "#2C3E50",
    "tab.inactiveForeground": "#95A5A6",
    "tab.unfocusedActiveBackground": "#34495E",
    "tab.unfocusedInactiveBackground": "#2C3E50",
    "terminal.foreground": "#ECF0F1",
    "titleBar.activeBackground": "#2C3E50",
    "titleBar.activeForeground": "#ECF0F1",
    "titleBar.inactiveBackground": "#34495E",
    "titleBar.inactiveForeground": "#95A5A6"
  },
  "tokenColors": [
    {
      "name": "Comentários",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#95A5A6"
      }
    },
    {
      "name": "Strings",
      "scope": ["string"],
      "settings": {
        "foreground": "#F39C12"
      }
    },
    {
      "name": "Funções",
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#27AE60"
      }
    },
    {
      "name": "Variáveis",
      "scope": ["variable"],
      "settings": {
        "foreground": "#3498DB"
      }
    },
    {
      "name": "Palavras-chave",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#E74C3C"
      }
    },
    {
      "name": "Constantes",
      "scope": ["constant"],
      "settings": {
        "foreground": "#9B59B6"
      }
    },
    {
      "name": "Números",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#F1C40F"
      }
    },
    {
      "name": "Delimitadores",
      "scope": ["punctuation.definition", "punctuation.separator", "punctuation.terminator"],
      "settings": {
        "foreground": "#ECF0F1"
      }
    }
  ]
}